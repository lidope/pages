.lead_previewImage
    width 100vw
    height 100vh
    background-color #000
    overflow hidden
    position fixed
    left 0
    top 0
    z-index 39999

    .lead_previewImage_page
        position absolute
        width 100%
        height 100px
        left 0
        top 0

        span
            text-align center
            color #fff
            font-size 34px

    .lead_previewImage_list
        max-width 100vw
        max-height 100vh

        img
            max-width 100vw
            max-height 100vh
